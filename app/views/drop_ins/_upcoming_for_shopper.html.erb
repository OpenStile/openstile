<div class="col-md-offset-1 col-xs-offset-1 col-md-1 col-xs-1 vcenter">
  <a id="cancel" data-confirm="Are you sure you want to remove this drop in?" 
     data-method="delete" 
     href=<%= drop_in_path(drop_in) %> 
     rel="nofollow"><i class="fa custom-fa-lg fa-minus-circle"></i></a>
</div>
<div class="col-md-3 col-xs-3 vcenter">
  <a href=<%= retailer_path(drop_in.retailer) %> >
    <%= cl_image_tag(drop_in.retailer.logo_image_name, 
                      class: "img-responsive",
                      width: 200,
                      alt: drop_in.retailer.name) %>
  </a>  
</div>
<div class="col-md-6 col-xs-6 vcenter">
  <div class="col-md-12 col-xs-12">
    <h4><%= drop_in.colloquial_time %></h4>
  </div>
  <div class="col-md-12 col-xs-12">
    <h4><%= drop_in.retailer.get_drop_in_location(drop_in.time.to_s).stringify %></h4>
  </div>
</div>
